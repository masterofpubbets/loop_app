,sum(x2.[Last 2 Weekly]) as ' + @colWeek2 + '

,sum(x2.[Last 3 Weekly]) as ' + @colWeek3 + '

,sum(x2.[Last 4 Weekly]) as ' + @colWeek4 + '

 

,'+ @colSUM +'

 

 

 

