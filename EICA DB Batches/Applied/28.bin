CREATE Proc [dbo].[SP_P6Summary_Est2]
AS
SELECT
tblActIDS.Team,tblActIDS.PCS_Area,tblActIDS.UOM,tblActIDS.ResourceName
,rtrim(PCS_Area) AS [PCS Area]
,EICA_Area
