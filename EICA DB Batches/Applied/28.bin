,[Qty Done] as EICA_Done
,tblActIDS.PCSDone AS [PCS Done %]
,weekly as EICA_Weekly
,Scope as [EICA Actual Scope]
FROM
(
select 
[Act ID]
,sum(Scope) as Scope
,sum([Qty Done]) as [Qty Done]
,sum(Weekly) as Weekly
from
(
select 
actid as [Act ID]
